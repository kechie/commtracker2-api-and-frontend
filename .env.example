# Example environment variables
# Database
DATABASE_URL=postgres://user:password@localhost:5432/commtracker2

# App
API_PORT=3007
NODE_ENV=development
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRATION=8h

# Static files served by nginx (no trailing slash)
# e.g. https://static.example.com/dts2
STATIC_BASE_URL=https://static.example.com/commtracker2/uploads

# Sequelize sync behavior (development/test only)
# Set to 'true' to force sync in dev/test (dangerous in prod)
SEQUELIZE_SYNC_FORCE=false
